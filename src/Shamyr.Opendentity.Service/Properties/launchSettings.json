{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Shamyr.Opendentity.Service": {
      "commandName": "Project",
      "environmentVariables": {
        "INIT_FILE_PATH": "InitData.json",
        "DATABASE_CONNECTION_STRING": "Host=localhost;Database=identity_opendentity;Username=admin;Password=password",
        "ACCESS_TOKEN_DURATION": "00:10:00",
